{
    "name": "warlof/seat-discord-connector",
    "description": "A SeAT plugin which maintain user access on a Discord Guild based on defined pattern.",
    "type": "seat-plugin",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Warlof\\Seat\\Connector\\Drivers\\Discord\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Warlof\\Seat\\Connector\\Drivers\\Discord\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-json": "*",
        "warlof/seat-connector": "^2.0",
        "socialiteproviders/discord": "^v2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Warlof Tutsimo",
            "email": "loic.leuilliot@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Warlof\\Seat\\Connector\\Drivers\\Discord\\DiscordConnectorServiceProvider"
            ]
        }
    }
}
